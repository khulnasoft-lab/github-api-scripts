name: github-api-scripts-workflow-upload-artifact
on:
  workflow_call:
  repository_dispatch:
    types: [custom_event_type]
jobs:
  github-api-scripts-job1:
    runs-on: github-api-scripts
    steps:
      - name: MyCommands
        run: |
          echo Hello
          mkdir -p test1/123
          touch test1/123/file1.txt
          ls -lr test1/123
      - name: upload
        uses: actions/upload-artifact@v3
        with:
            name: my-artifact
            path: |
              test1/**/*.txt
